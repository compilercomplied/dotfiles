{

    "coc.preferences.triggerSignatureHelp": false,

    "codeLens.enable": true,
    "codeLens.separator": "‣",

    "suggest.noselect": false,
    "suggest.detailField": "abbr",
    "suggest.autoTrigger": "always",
    "suggest.triggerAfterInsertEnter": true,
    "suggest.maxCompleteItemCount": 50,
    "suggest.fixInsertedWord": true,
    "suggest.snippetIndicator": " ►",
    "suggest.reloadPumOnInsertChar": true,
    "suggest.enablePreview": true,
    "suggest.echodocSupport": true,

    "tsserver.enable": true,

    "pyls.enable": true,
    "pyls.commandPath": "",    // Remember you can use microsoft's python LSP instead

    "rust.trace.server": "verbose",
    "rust-client.disableRustup": false,
    "rust-client.updateOnStartup": true,
    "rust.build_on_save": true,
    "rust-client.channel": "stable",
    "rust.unstable_features": true
    
}
